{
  "window.zoomLevel": 1,
  "files.autoSaveDelay": 2,
  "files.autoSave": "afterDelay",
  "explorer.confirmDelete": false,
  "explorer.confirmDragAndDrop": false,
  "explorer.confirmMoveToTrash": false,
  "promptOnDeleteFile": false,
  "extensions.ignoreRecommendations": true,
  "editor.fontSize": 13,
  "editor.bracketPairColorization.enabled": false,
  "editor.quickSuggestionsDelay": 0,
  "workbench.activityBar.location": "hidden",
  "editor.fontFamily": "JetBrains Mono",
  "editor.fontLigatures": true,
  "editor.cursorBlinking": "solid",
  "workbench.colorCustomizations": {
  "editorCursor.foreground": "#3085c7",
    
    "terminalCursor.foreground": "#3085c7"
  },
  "vim.insertModeKeyBindings": [
    {
      "before": ["j", "k"],
      "after": ["<Esc>"]
    }
  ],
  "workbench.colorTheme": "06143cbe-2d51-4423-9a93-73a02c828119",
  "terminal.integrated.fontSize": 13,
  "editor.minimap.renderCharacters": false,
  "editor.minimap.enabled": false,
  "terminal.integrated.enableMultiLinePasteWarning": "never",
  "terminal.external.osxExec": "iTerm.app",
  "terminal.explorerKind": "external",
  "workbench.statusBar.visible": false,
  "workbench.iconTheme": "material-icon-theme",
  "editor.glyphMargin": false,
  "terminal.integrated.customGlyphs": false,
  "diffEditor.renderMarginRevertIcon": false,
  "terminal.integrated.rescaleOverlappingGlyphs": true,
  "editor.folding": false,
  "editor.foldingHighlight": false,
  "editor.guides.indentation": false,
  "workbench.tree.renderIndentGuides": "none",
  "workbench.editor.showTabs": "single",
  "workbench.secondarySideBar.defaultVisibility": "hidden",
  "workbench.sideBar.location": "right",
  "workbench.editor.showIcons": true,
  "workbench.editor.labelFormat": "medium",
  "security.workspace.trust.untrustedFiles": "open",
  "security.workspace.trust.banner": "never",
  "editor.gotoLocation.multipleDefinitions": "goto",
  "breadcrumbs.enabled": false,
  "chat.commandCenter.enabled": false,
  "workbench.layoutControl.enabled": false,
  "workbench.navigationControl.enabled": false,
  "window.commandCenter": false,
  "material-icon-theme.hidesExplorerArrows": true,
  "flow-icons.hidesExplorerArrows": true
}
